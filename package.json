{
    "name": "apartment-finder",
    "description": "Finding a place to live, using AI recommendations.",
    "version": "0.1.0",
    "type": "module",
    "private": true,
    "devDependencies": {
        "@eslint/js": "^9.12.0",
        "@typescript-eslint/eslint-plugin": "^7.18.0",
        "@typescript-eslint/parser": "^7.18.0",
        "eslint": "^9.12.0",
        "eslint-config-airbnb-typescript": "^18.0.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-react": "^7.37.1",
        "eslint-plugin-react-refresh": "^0.4.12",
        "globals": "^15.10.0",
        "prettier": "^3.3.3",
        "typescript": "^4.9.5",
        "typescript-eslint": "^8.8.0"
    },
    "dependencies": {
        "react": "^18.3.1",
        "react-dom": "^18.3.1",
        "react-scripts": "^5.0.1",
        "web-vitals": "^2.1.0",
        "yarn": "^1.22.22"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "format:check": "prettier --check 'src/**/*.{js,jsx,ts,tsx,json,md}'",
        "format:write": "prettier --write 'src/**/*.{js,jsx,ts,tsx,json,md}'",
        "types:check": "tsc --noEmit",
        "lint": "eslint src/**.ts src/*.tsx",
        "lint:fix": "eslint src/**/*.ts src/**/*.tsx --fix"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/roberto-pano/apartment-finding.git"
    },
    "homepage": "https://github.com/roberto-pano/apartment-finding#readme"
}
